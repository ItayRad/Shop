
<%- include("partials/header"); -%>
    <h1>Register</h1>
    <form class="" action="/register" method="post">
      <div class="form-group">
        <label>Email*</label>
        <input type="email" style="width:25%" maxlength="35" class="form-control" name="email">
        <label>User*</label>
        <input class="form-control" maxlength="15" style="width:15%" type="text" name="username">
        <label>Password</label>    <i   onclick="togglePasswordFieldClicked()" id="togglePasswordField" value="Toggle Password" class="fas fa-eye"></i>

        <input class="form-control" style="width:15%" id="passwordField" type="password"  value="" autocomplete="off" name="password">

      </div>
      <button class="btn btn-primary" type="submit" name="button">Register</button>
    </form>






    <div id="output"></div>
    <script src="bundle.js"></script>
<script src="javascripts/main.js"></script>
<%- include("partials/footer"); -%>
